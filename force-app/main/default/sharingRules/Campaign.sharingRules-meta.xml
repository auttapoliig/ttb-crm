    <sharingCriteriaRules>
        <fullName>Share_Active_and_Expire_Campaign</fullName>
        <accessLevel>Read</accessLevel>
        <label>Share Active and Expire Campaign</label>
        <sharedTo>
            <roleAndSubordinates>CEO</roleAndSubordinates>
        </sharedTo>
        <criteriaItems>
            <field>RecordTypeId</field>
            <operation>notEqual</operation>
            <value>Commercial,Dummy Campaign</value>
        </criteriaItems>
        <criteriaItems>
            <field>Status</field>
            <operation>equals</operation>
            <value>On Hold,Open,In Progress,Expired</value>
        </criteriaItems>
        <includeRecordsOwnedByAll>false</includeRecordsOwnedByAll>
    </sharingCriteriaRules>
