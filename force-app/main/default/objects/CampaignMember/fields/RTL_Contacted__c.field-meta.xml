<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>RTL_Contacted__c</fullName>
    <description>contact status = contact or follow-up</description>
    <externalId>false</externalId>
    <formula>IF( OR( CONTAINS(TEXT( RTL_Contact_Status__c), &apos;Contact&apos;), 

TEXT(RTL_Contact_Status__c ) = &apos;Follow-up&apos;) , 1,0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Contacted</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
