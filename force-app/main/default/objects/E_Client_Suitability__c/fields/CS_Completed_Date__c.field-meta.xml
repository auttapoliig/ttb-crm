<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CS_Completed_Date__c</fullName>
    <externalId>false</externalId>
    <formula>IF(ISPICKVAL(CS_Status__c, &quot;Completed&quot;), 
			IF(CS_Basic_Flag__c, LastModifiedDate ,
						IF(CS_SIgn_Date__c == DATEVALUE(LastModifiedDate), LastModifiedDate,DATETIMEVALUE(CS_SIgn_Date__c)) ) ,null)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>CS Completed Date</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>DateTime</type>
</CustomField>
