<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>SLA_Calculate__c</fullName>
    <externalId>false</externalId>
    <formula>IF(AND(OR(ISPICKVAL(Severity__c,&apos;Critical&apos;),ISPICKVAL(Severity__c,&apos;High&apos;)),Age__c&gt;1),0,
IF(AND(ISPICKVAL(Severity__c,&apos;Medium&apos;),Age__c&gt;2),0,
IF(AND(ISPICKVAL(Severity__c,&apos;Low&apos;),Age__c&gt;3),0,1)))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>SLA Calculate</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
