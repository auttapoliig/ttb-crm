<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>OneApp_Status__c</fullName>
    <description>transform case status to oneapp status</description>
    <externalId>false</externalId>
    <formula>if(!ISPICKVAL(Status, &apos;Completed&apos;) &amp;&amp; !ISPICKVAL(Status, &apos;Cancel&apos;)
		,&apos;In progress&apos;
		,IF(ISPICKVAL(Status, &apos;Completed&apos;)
			,&apos;Closed&apos;
		 ,&apos;-&apos;))</formula>
    <label>OneApp Status</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
