<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Is_My_Branch__c</fullName>
    <description>Check if User&apos;s branch code match with branch code in case object</description>
    <externalId>false</externalId>
    <formula>OR(CONTAINS( Owner:Queue.DeveloperName , $User.RTL_Branch_Code__c ), Owner:User.RTL_Branch_Code__c =  $User.RTL_Branch_Code__c) &amp;&amp; NOT( ISBLANK( $User.RTL_Branch_Code__c ) )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Is My Branch</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
