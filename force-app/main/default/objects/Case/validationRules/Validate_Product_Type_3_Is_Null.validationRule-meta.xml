<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Validate_Product_Type_3_Is_Null</fullName>
    <active>true</active>
    <description>When product number 3 has value,Product type 3 must not be null.</description>
    <errorConditionFormula>AND(ISBLANK(TEXT(Product_Type_3__c)), (Product_Number_3__c!=null ))</errorConditionFormula>
    <errorDisplayField>Product_Type_3__c</errorDisplayField>
    <errorMessage>Please input value</errorMessage>
</ValidationRule>
