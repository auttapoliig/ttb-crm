<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Validate_Product_Type_1_Is_Null</fullName>
    <active>true</active>
    <description>When product number 1 has value,Product type 1 must not be null.</description>
    <errorConditionFormula>AND(ISBLANK(TEXT(Product_Type_1__c)), (Product_Number_1__c!=null ))</errorConditionFormula>
    <errorDisplayField>Product_Type_1__c</errorDisplayField>
    <errorMessage>Please input value</errorMessage>
</ValidationRule>
