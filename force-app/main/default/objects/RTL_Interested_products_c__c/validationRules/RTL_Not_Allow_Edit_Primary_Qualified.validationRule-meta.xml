<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>RTL_Not_Allow_Edit_Primary_Qualified</fullName>
    <active>false</active>
    <errorConditionFormula>AND( 
ISPICKVAL(Lead__r.Status,&apos;Qualified&apos;), 
ISCHANGED( Is_Primary__c ) 
)</errorConditionFormula>
    <errorMessage>Primary Interested Product cannot be created or modified when Lead is Qualified</errorMessage>
</ValidationRule>
