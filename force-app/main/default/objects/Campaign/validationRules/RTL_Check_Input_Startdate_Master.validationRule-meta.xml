<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>RTL_Check_Input_Startdate_Master</fullName>
    <active>true</active>
    <description>Preventing incorrect input startdate</description>
    <errorConditionFormula>OR( StartDate &gt; (TODAY() +(365.2425*50)) , 

StartDate  &lt; (TODAY() - (365.2425*50 )))</errorConditionFormula>
    <errorDisplayField>StartDate</errorDisplayField>
    <errorMessage>Invalid Startdate</errorMessage>
</ValidationRule>
