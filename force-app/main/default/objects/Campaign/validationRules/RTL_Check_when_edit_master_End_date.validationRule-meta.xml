<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>RTL_Check_when_edit_master_End_date</fullName>
    <active>true</active>
    <errorConditionFormula>AND(  Contains(RecordType.Name, &apos;Master Campaign&apos;) , 
      EndDate &lt;  Stamp_Child_End_Date__c )</errorConditionFormula>
    <errorDisplayField>EndDate</errorDisplayField>
    <errorMessage>The End date of master campaign should be greater than the child campaign end date.</errorMessage>
</ValidationRule>
