<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>RTL_Check_Input_Enddate_Master</fullName>
    <active>true</active>
    <description>Preventing incorrect input enddate</description>
    <errorConditionFormula>OR( EndDate &gt; (TODAY() +(365.2425*50)) , 

 EndDate  &lt; (TODAY() - (365.2425*50 )))</errorConditionFormula>
    <errorDisplayField>EndDate</errorDisplayField>
    <errorMessage>Invalid Enddate</errorMessage>
</ValidationRule>
