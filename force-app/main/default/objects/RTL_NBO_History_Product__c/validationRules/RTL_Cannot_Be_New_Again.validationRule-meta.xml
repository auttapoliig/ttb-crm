<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>RTL_Cannot_Be_New_Again</fullName>
    <active>true</active>
    <description>NBO Product Status cannot be New from Accepted or Rejected</description>
    <errorConditionFormula>OR(AND( ISPICKVAL(PRIORVALUE(RTL_Status__c), &quot;Rejected&quot;) , ISPICKVAL(RTL_Status__c,&quot;New&quot;)), AND(ISPICKVAL(PRIORVALUE(RTL_Status__c), &quot;Accepted&quot;), ISPICKVAL(RTL_Status__c,&quot;New&quot;)))</errorConditionFormula>
    <errorMessage>NBO Product Status cannot revert to New from Accepted or Rejected</errorMessage>
</ValidationRule>
