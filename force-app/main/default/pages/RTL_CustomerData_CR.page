    <script>

        function startLoading(text) {
            //$('#load_scrl').css('top', $(document).scrollTop() + 200).html(text);
            $('.loadingBox').show();
        }
        function endLoading() {
            $('.loadingBox').hide();
        }



        function redirectExternalURL(url, pageName) {
            if (sforce.console.isInConsole()) {
                // In Sales Console
                //OpenTabPage(url,pageName);
                //the external URL must be whitelisted domain in "retail sales console"
                //use null (_primaryTabId) to open a new primary tab
                //sforce.console.openPrimaryTab(null , url, true, pageName,openSuccess);
                window.open(url, '_blank');
            } else if ((typeof sforce != 'undefined') && sforce && (!!sforce.one)) {
                //In Salesforce1
                sforce.one.navigateToURL(url);
            } else {
                //Desktop Version
                //window.location.href = url;
                window.open(url, '_blank');
            }
        }

        $("body").on("click", "a[id^=lookup]", function () {
            event.preventDefault();
            var objId = this.id.substring(6, 21);
            var tabName = '';
            redirectURL(objId, tabName);
        });

        function redirectURL(url, pageName) {
            if (sforce.console.isInConsole()) {
                // In Sales Console
                openTab(url, '/', pageName);
            } else if ((typeof sforce.one != 'undefined') && (sforce.one != null)) {
                //In Salesforce1
                // openTab(url,'/',pageName); // open Popup
                sforce.one.navigateToURL('/' + url); //active link
            } else {
                //Desktop Version
                window.parent.location.replace('/' + url);
            }
        }

        var _navigateToUrl = window.navigateToUrl;
        var navigateToUrl = function (url, data1, data2) {
            if (sforce.console.isInConsole()) {
                tabName = '';
                // Always open in primary tab
                //sforce.console.openPrimaryTab(undefined, url , true,tabName, null, 'salesforceTab');
                openPrimaryTab(undefined, url, true, tabName, callbackOpenTab, 'salesforceTab');
            }
            else {
                _navigateToUrl(url, data1, data2);
            }
        }

        function openPrimaryTab(id, url, active, tabLabel, callback, name) {
            // primaryTabName = name;

            sforce.console.openPrimaryTab(id, url, active, tabLabel, callback, name);
        }

        var callbackOpenTab = function callbackOpenTab(result) {
            sforce.console.setTabIcon("{!URLFOR($Resource.RTL_Campaign_Logo)}", result.id);

        }

        function openTab(recid, url, tabName) {
            if ((typeof sforce.one != 'undefined') && (sforce.one != null)) {
                sforce.one.navigateToURL(url + recid);
            }
            else if (!sforce.console.isInConsole()) {
                window.open(url + recid, '_top');
            }
            else {
                // Always open in primary tab
                // sforce.console.openPrimaryTab(undefined, url+recid, true,tabName, null, 'salesforceTab'+recid);
                openPrimaryTab(null, url + recid, true, tabName, callbackOpenTab, 'salesforceTab' + recid);
            }
        }       

        $("body").on("click", ".userProfileHoverContent a[href^=http]", function () {
            var href = $(this).attr('href');
            var URL = href.split("/");
            var id = URL[URL.length-1];
            $(this).attr('href','#');
            $(this).attr('alt',id);
            
            redirectURL(id, $(this).text());
        });

        $("body").on("click", ".userProfileHoverContent a[href^=#]", function () {
            var id = $(this).attr('alt');
            
            redirectURL(id, $(this).text());
        });

        $(document).ready(function () {
            
            $('.loadingBox').hide();

            if (jQuery(document).height() <= jQuery(window).height()){
                jQuery('.More').fadeOut("slow");
            }
            
            window.addEventListener('touchmove', function() {
                if (jQuery(document).height() <= (jQuery(window).height() + jQuery(window).scrollTop())) {
                    jQuery('.More').fadeOut("slow");
                }else{
                    jQuery(".More").fadeIn("slow");
                }                    
            });
            
            jQuery(window).scroll(function() {
                if (jQuery(document).height() <= (jQuery(window).height() + jQuery(window).scrollTop()+20)) {
                    jQuery('.More').fadeOut("slow");            
                }else{
                    jQuery(".More").fadeIn("slow");
                }
            });

            // Call_OSC07();
            CallRest_OSC07();
    });

    //Call Data partition after success call OSC07
    function callDataPartition(respon)
    {       
            if ({!isSF1}){
                loadDataSaleSupportMobile();
                loadDataMIMobile();
                loadDataCRMobile();
                loadDataCRDMobile();
                loadDataCPHMobile();
            }else {
                loadDataSaleSupport();
                loadDataMI();
                loadDataCR();
                loadDataCRD();
                loadDataCPH();
            }   
    }
