                /* font-size: 14px; */
                color: #080807;
                width: 100%;
                float: left;
                padding-left: 10px;
                margin-top: 0;
                margin-bottom: 5px;
                font-family: 'Salesforce Sans', Arial, sans-serif;
                text-transform: uppercase;
                line-height: 1.875rem;
                font-weight: inherit;
                background-color: #ececec;
                display: block;
            }

            @media (max-width: 670px) {
                body {
                    background: #fff;
                    overflow-x: scroll !important;
                }
                .bootstrap-sf1 {
                    margin: 0px auto;
                    border: none;
                }
                .activitySection {
                    clear: both;
                    width: 100%;
                }
                .revenueSection {
                    width: 100%;
                }
                .col {
