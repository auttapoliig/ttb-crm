            }

            .closebtn {
                text-align: center;
                margin-bottom: 20px !important;
            }

            .reportClick {
                top: 0;
                width: 285px;
                height: 235px;
                position: absolute;
                z-index: 102;
                background-image: url('{!$Resource.transparent}');
            }

            .activityPipelineSection {
                height: 600px;
            }

            .activitySection {
                font-size: 80%;
                width: 49%;
            }

            .revenueSection {
                font-size: 80%;
                width: 65%;
            }

            .col {
                width: 50%;
                float: left;
            }

            .CheckMobile {
                display: block;
                width: 98%;
                float: left;
                padding-left: 10px;
            }

            .SessionMobile {
