                font-size: 15px;
                color: black;
                margin: 0;
                padding-bottom: 3px;
                display: block;
                min-width: 100px;

            }

            .sf1label {
                font-family: 'ProximaNovaSoft-Regular', 'Helvetica Neue';
                font-size: 13px;
                color: #696e71;
                margin: 0;
                padding-bottom: 3px;
                display: block;
                min-width: 100px;
            }

            textarea {
                resize: none;
            }

            .hasMotif {
                margin: 0;
                margin-left: -10px;
            }

            .bPageBlock.brandSecondaryBrd.bDetailBlock.secondaryPalette {
                margin-bottom: 0;
            }

            @media (max-width: 1024px) {
                .col {
                    width: 50%;
                    float: left;
                }
                .CheckMobile {
                    display: none;
                }
                .CheckTablet {
                    width: 98%;
                    float: left;
                    padding-left: 10px;
                }
                .CheckLine {
                    border-bottom: 1px solid #e3deb8;
                    border-color: #ececec;
                    display: Flex;
                }
                .CheckPosition {
                    padding-left: 30px;
                    margin-left: 30px;
                }
                .Session {
                    font-size: .625rem;
                    padding-left: 10px;
                    text-transform: uppercase;
                    line-height: 1.875rem;
                    font-weight: inherit;
                    background-color: #ececec;
                    ;
                    display: block;
                }
                .SessionMobile {
                    display: none;

                }
                .More {
                    padding-top: 5px;
                    background-color: #ccc;
                    text-align: center;
                    position: fixed;
                    bottom: 0;
                    height: 25px;
                    width: 100% !important;
                }
            }

            @media (max-width: 670px) {
                .col {
                    width: 100%;
                }
                .CheckTablet {
                    display: none;
                }
                .CheckLine {
                    display: none;
                }
                .CheckMobile {
                    width: 100%;
                    float: left;
                    padding-left: 10px;
                    border-bottom: 1px solid #e3deb8;
                    border-color: #ececec;
                    display: block;
                }
                .SessionMobile {
                    width: 100%;
                    float: left;
                    padding-left: 10px;
                    font-family: 'Salesforce Sans', Arial, sans-serif;
                    font-size: .625rem;
                    text-transform: uppercase;
                    line-height: 1.875rem;
                    font-weight: inherit;
                    background-color: #ececec;
                    /*background-color: rgb(243, 242, 242);*/
                    display: block;
                }
                .Session {
                    display: none;
                }
            }

            .fontstyle {
                /* font-size: 10.35px;
                font-weight: bold;
                font-family: Arial, Helvetica, sans-serif; */
            }

            body .bPageBlock .pbBody .pbSubheader {
                margin-top: 10px;
            }
            body .bPageBlock .detailList .labelCol, .slds-vf-scope .bPageBlock .detailList .labelCol {
