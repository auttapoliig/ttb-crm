        <apex:includescript value="{!URLFOR($Resource.TMBjquery, '/TMBassets/jquery/jquery-1.11.2.min.js')}" />
        <style type="text/css">
            span{
                color: #080707 ; 
            }
            img {
                color: #080707 !important;
            }
            
            h3 {
                font-size: 14px !important;
                color: #080707 !important;
            }

            .sf1value {
