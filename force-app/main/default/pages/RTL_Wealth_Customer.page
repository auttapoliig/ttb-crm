            if(_isInConsole){
                // In Sales Console
                OpenTabPage(url,pageName);

            }else if(_isInSF1){
                //In Salesforce1
                sforce.one.navigateToURL(url);
            }else{
                //Desktop Version
                window.open(url,'_blank');
            }
            //endLoading();
     }
