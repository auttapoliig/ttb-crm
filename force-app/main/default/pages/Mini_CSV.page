        <script type="text/javascript">
            function getMobileOperatingSystem() {
                var userAgent = navigator.userAgent || navigator.vendor || window.opera;
                if (userAgent.match(/iPad/i) || userAgent.match(/iPhone/i) || userAgent.match(/iPod/i)) {
                    return 'iOS';
                } else if (userAgent.match(/Android/i)) {
                    return 'Android';
                } else {
                    return 'unknown';
                }
            }

            function goToOutside() {
                switch (getMobileOperatingSystem()) {
                    case 'Android':
                        sforce.one.navigateToURL('{!Deeplink_Android}');
                        break;
                    case 'iOS':
                        sforce.one.navigateToURL('{!Deeplink_iOS}');
                        break;
                    default:
                        console.log('Unknown');
                        break;
                }
            }

            function getLocation(href) {
                var l = document.createElement("a");
                l.href = href;
                return l;
            };

            function goToKnowledgeCenter(deeplink) {
                var location = getLocation(deeplink);
                if (deeplink || location.pathname || location.hash) {
                    sforce.one.navigateToURL(deeplink);
                }
                else {
                    alert('เกิดข้อผิดพลาด ไม่สามารถเปิดไฟล์ที่คุณต้องการได้');
                }
            }

            function redirectBack() {
                sforce.one.navigateToURL('{!retURL}');
            }

            $(document).ready(function () {
                // Important for call out enlite service
                if ('{! account.Customer_Type__c }' == 'Juristic') {
                    calloutStartCompanyListAndDetailService();
                }
            });

        </script>

