                                                                    <apex:outputField value="{!acctplanwallet.WalletNiid_CashOnHand__c}"
                                                                        styleClass="form-control" />
                                                                </td>
                                                            </tr>
                                                        </table>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        

