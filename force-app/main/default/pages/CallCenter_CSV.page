                        sforce.console.setTabIcon("{!URLFOR($Resource.customer_logo_icon)}", result.id);
                    }
                });
                CallWealthData();

                overPopup("mainCase", "refCase");
                overPopup("mainVerificationHistory", "refVerificationHistory");
                overPopup("mainDoNotContact", "refDoNotContact");
                overPopup("mainComplaint", "refComplaint");
                overPopup("mainOpportunity", "refOpportunity");
                overPopup("mainReferral", "refReferral");
                overPopup("mainOpenActivity", "refOpenActivity");
                overPopup("mainActivityHistory", "refActivityHistory");
                overPopup("mainInterestProduct", "refInterestProduct");
                overPopup("mainFNAActivity", "refFNAActivity");
                overPopup("mainAccountHistory", "refAccountHistory");
                overPopup("mainNoteAndAttach", "refNoteAndAttach");

                overPopup("fieldWealth", "tooltipWealth");
                overPopup("fieldMobile", "tooltipMobile");
                overPopup("fieldIdType", "tooltipIdType");
                overPopup("fieldPrimaryBanking", "tooltipPrimaryBanking");
                overPopup("fieldAverageAUM", "tooltipAverageAUM");
                overPopup("fieldMarketConsent", "tooltipMarketConsent");

                //Modify-- SCR0281148 CRs (Retail) : Enhance fieldname and position in CRM Screen>
                overPopup("fieldRTL_Fund_Risk_Mismatch__c", "tooltipRTL_Fund_Risk_Mismatch__c");
                overPopup("fieldRTL_Fund_High_Concentration_Risk__c", "tooltipRTL_Fund_High_Concentration_Risk__c");
                //Modify-- SCR0281148 CRs (Retail) : Enhance fieldname and position in CRM Screen>

                $("a[id^=lookup]").on('click', function (event) {
                    if (sforce.console.isInConsole()) {
                        event.preventDefault();
                        var objId = this.id.substring(6, 21);
                        //console.log('objId'+objId);
                        var tabName = '';
                        openTab(objId, '/', tabName);
                    }

                });

            });

            function overPopup(main, ref) {

                $("#" + main).mouseover(function () { $("#" + ref).css('display', 'block'); });
                $("#" + main).mouseout(function () { $("#" + ref).css('display', 'none'); });
                $("#" + ref).mouseover(function () { $("#" + ref).css('display', 'block'); });
                $("#" + ref).mouseout(function () { $("#" + ref).css('display', 'none'); });


            }

            function moveToRelatedList(tabName) {
                $('html, body').animate({
                    scrollTop: $("#" + tabName).offset().top
                }, 500);
                $("#" + tabName).removeClass("slds-is-open").addClass("slds-is-open");
                $("#" + tabName + "_Content").show();
            }

        </script>

