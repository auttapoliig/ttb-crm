    <rules>
        <fullName>Update to-be fields%28Change Owner Cross Segment%29</fullName>
        <actions>
            <name>Stamp_To_Be_New_Owner_Team</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Stamp_To_be_New_Owner_Name_Account</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Stamp_to_be_New_Owner_Id_Account</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Stamp_to_be_New_Owner_Name</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Stamp_to_be_New_Segment_Account</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Stamp_to_be_New_Segment_Approval_history</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>AND(ISPICKVAL(Change_Owner_Approval_Status__c, &quot;Final Approved&quot;) , Change_Owner_Cross_Segment_Flag__c = TRUE)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Update to-be fields%28Request Change Segment%29</fullName>
        <actions>
            <name>Stamp_Change_Segment_Reason</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Stamp_to_be_New_Segment_Account</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Change_Owner_Approval_History__c.Change_Owner_Approval_Status__c</field>
            <operation>equals</operation>
            <value>Final Approved</value>
        </criteriaItems>
        <criteriaItems>
            <field>Change_Owner_Approval_History__c.RecordTypeId</field>
            <operation>equals</operation>
            <value>Change Segment</value>
        </criteriaItems>
        <triggerType>onAllChanges</triggerType>
    </rules>
