        <fullName>Update_Expiry_Date</fullName>
        <description>Update Expiry Date after CS is completed</description>
        <field>CS_Expiry_Date_Next__c</field>
        <formula>DATE(
    YEAR( TODAY() ) + 1 ,
    Month( TODAY()),
    CASE( MONTH(TODAY()), 
        1, 31, 
        2, IF( MOD( YEAR(TODAY())+1 , 4) = 0, 29, 28), 
        3, 31, 
        4, 30, 
        5, 31, 
        6, 30, 
        7, 31, 
        8, 31, 
        9, 30, 
        10, 31, 
        11, 30, 
        12, 31, 
        0 
    )
)</formula>
        <name>Update Expiry Date</name>
        <notifyAssignee>false</notifyAssignee>
