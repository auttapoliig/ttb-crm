    <rules>
        <fullName>CS Alert Unfreeze Limit for Client Suitability</fullName>
        <actions>
            <name>CS_Alert_to_Unfreeze_Limit_for_Client_Suitability</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <booleanFilter>(1 AND 2 AND 3) OR (3 AND 4)</booleanFilter>
        <criteriaItems>
            <field>E_Client_Suitability__c.CS_New_Customer_flag__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <criteriaItems>
            <field>E_Client_Suitability__c.CS_Cancelled_By__c</field>
            <operation>equals</operation>
        </criteriaItems>
        <criteriaItems>
            <field>E_Client_Suitability__c.CS_Status__c</field>
            <operation>equals</operation>
            <value>Completed</value>
        </criteriaItems>
        <criteriaItems>
            <field>E_Client_Suitability__c.Is_Overdue__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <description>New customer and status = completed and CS was overdue and complete</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>CS Auto Checkbox Overdue</fullName>
        <actions>
            <name>Update_Is_Overdue</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <description>auto make checkbox to true if record&apos;s overdue</description>
        <formula>IF( ( CS_Expiry_Date_Next__c &lt; TODAY() &amp;&amp; (NOT( ISPICKVAL( CS_Status__c , &quot;Completed&quot;)) || NOT( ISPICKVAL( CS_Status__c , &quot;Suspended&quot;)))),True,False )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>CS Email Alert Reverse Form A</fullName>
        <actions>
            <name>CS_Alert_to_RM_Reverse_Form_A</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <formula>AND(     ISCHANGED(CS_Status__c),     OR( ISPICKVAL(PRIORVALUE(CS_Status__c),&quot;Pending Sales&quot;), ISPICKVAL(PRIORVALUE(CS_Status__c),&quot;Sales TH Rejected&quot;)),     ISPICKVAL(CS_Status__c, &quot;Reviewing&quot;) )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>CS Stamp RM TH Info</fullName>
        <actions>
            <name>CS_Stamp_RM_TH</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>CS_Stamp_RM_TH_Email</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>ISCHANGED( Select_RM_TH_Approver__c )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>CS Update status to Pending Sale</fullName>
        <actions>
            <name>Change_CS_status_to_Pending_Sales</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>(in case of BB,SE)</description>
        <formula>ISPICKVAL( CS_Customer_Survey_A__r.Status__c , &quot;Ready for Review&quot;)  &amp;&amp; RecordType.DeveloperName = &quot;Reviewing&quot; &amp;&amp; (CS_Customer_Segment__c = &quot;BB&quot; || CS_Customer_Segment__c = &quot;SE&quot; )</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>CS auto overdue</fullName>
        <actions>
            <name>Status_to_Overdue</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>IF( ( CS_Expiry_Date_Next__c &lt; TODAY() &amp;&amp;  (NOT( ISPICKVAL( CS_Status__c , &quot;Completed&quot;)) || NOT( ISPICKVAL( CS_Status__c , &quot;Suspended&quot;)))),True,False )</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>CS update Sales TH Name</fullName>
        <actions>
            <name>Stamp_Sales_TH_Name</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>update Sales TH info if Sales is changed</description>
        <formula>ISCHANGED( CS_Sales_Owner__c )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Cancel to Basic</fullName>
        <actions>
            <name>Clear_CS_Expiry_Date</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Clear_CS_Sign_Date</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>When CS is cancelled status to completed and level to Basic</description>
        <formula>ISPICKVAL( CS_Status__c , &quot;Completed&quot;) &amp;&amp;  NOT( ISBLANK( CS_Cancelled_By__c ) )</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Change Status to Suspended</fullName>
        <actions>
            <name>Update_Record_Type_to_Suspended</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Update_Status_to_Suspended</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>E_Client_Suitability__c.CS_Suspend_flag__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <description>check &apos;Is Suspend&apos; then change status of e-CS status to Suspended</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Completed Stamp CS SignDate and Expiry Date%28Next%29</fullName>
        <actions>
            <name>Update_CS_Sign_Date</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Update_Expiry_Date</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>stamp CS sign Date &amp;&amp; Expiry Date (Next) after the CS is completed</description>
        <formula>ISPICKVAL( CS_Status__c , &quot;Completed&quot;) &amp;&amp;   ISBLANK( CS_Cancelled_By__c ) &amp;&amp;  NOT(ISPICKVAL( CS_Level__c , &quot;Basic&quot;) )</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>E-Client Suitability Schedule Expired</fullName>
