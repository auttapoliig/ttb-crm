        <description>stamp Sales TH name into the Sales TH approved date field</description>
        <field>CS_Sales_TH_Approved_Date_Time__c</field>
        <formula>$User.FirstName +&quot; &quot;+ $User.LastName + &quot;, &quot; + LPAD(TEXT(DAY(TODAY())),2,&apos;0&apos;)+&quot;/&quot;+ 
LPAD(TEXT(MONTH(TODAY())),2,&apos;0&apos;)+&quot;/&quot;+TEXT(YEAR(TODAY()))+&quot;, &quot;+MID(TEXT( ( NOW()) + (7/24) ),12,LEN(TEXT(NOW()))-15)</formula>
        <name>Update Sales TH approver</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Formula</operation>
        <protected>false</protected>
        <reevaluateOnChange>false</reevaluateOnChange>
