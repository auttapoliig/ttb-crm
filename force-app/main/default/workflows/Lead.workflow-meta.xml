    <rules>
        <fullName>Email Notification when change owner</fullName>
        <actions>
            <name>Email_alert_to_lead_owner</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <formula>AND(         OR(RecordType.Name = &quot;Commercial Account&quot;,          RecordType.Name = &quot;Commercial Completed&quot;,          RecordType.Name = &quot;Commercial Lead&quot;) , ISCHANGED( OwnerId ))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>RTL Blank Out Uncontact reason</fullName>
        <actions>
            <name>RTL_Blank_Uncontact_Reason</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Lead.Status</field>
            <operation>notEqual</operation>
            <value>Uncontacted</value>
        </criteriaItems>
        <criteriaItems>
            <field>Lead.RTL_Uncontact_reason__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Lead.RecordTypeId</field>
            <operation>equals</operation>
            <value>Retail Banking</value>
        </criteriaItems>
        <description>Blank out uncontact Reason when Status is not equal to uncontact.</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>RTL Set Lead Record type to Read Only</fullName>
        <actions>
            <name>RTL_Set_Lead_Record_Type</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Lead.Status</field>
            <operation>equals</operation>
            <value>Closed Lost,Duplicated</value>
        </criteriaItems>
        <criteriaItems>
            <field>Lead.RecordTypeId</field>
            <operation>contains</operation>
            <value>Retail</value>
        </criteriaItems>
        <description>WF rule is used to make all fields read only on leads upon Closed Lost status.</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
