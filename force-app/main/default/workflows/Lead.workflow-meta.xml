        </actions>
        <active>true</active>
        <formula>AND( OR( RecordType.Name = &quot;Commercial Account&quot;,RecordType.Name = &quot;Commercial Completed&quot;,RecordType.Name = &quot;Commercial Lead&quot;) ,ISBLANK( Account__c ),   TEXT(Customer_Type__c) = &quot;Individual&quot;,  ISBLANK(Sales_amount_per_year__c))</formula>
