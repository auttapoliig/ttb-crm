        <formula>AND( OR( RecordType.Name  = &quot;Commercial Account&quot;,RecordType.Name  = &quot;Commercial Completed&quot;,RecordType.Name  = &quot;Commercial Lead&quot;) ,NOT( ISBLANK( Account__c ) ))</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
