        </actions>
        <active>true</active>
        <formula>AND( OR( RecordType.Name = &quot;Commercial Account&quot;,RecordType.Name = &quot;Commercial Completed&quot;,RecordType.Name = &quot;Commercial Lead&quot;)  ,ISBLANK( Account__c ),       TEXT(Customer_Type__c) = &quot;Juristic&quot;,               ISBLANK(Group__c),                      AND( Industry__r.Special__c = false ,             ISBLANK( Sales_amount_per_year__c ) )      )</formula>
