    <aura:attribute name="recordId" type="Id" />
    <aura:attribute name="recordTypeId" type="Id" />
    <aura:attribute name="recordTypeIdString" type="String" default="" />
    <aura:attribute name="PrePopLeadRec" type="Lead" default="{ 'sobjectType': 'Lead' }" />
    <aura:attribute name="LeadRec" type="Lead" default="{ 'sobjectType': 'Lead' }" />
    <aura:attribute name="AccRec" type="Account" default="{ 'sobjectType': 'Account' }" />
    <aura:attribute name="RefRec" type="RTL_Referral__c" default="{ 'sobjectType': 'RTL_Referral__c' }" />
    <aura:attribute name="testUrl" type="String" default="" />
    <aura:attribute name="AccountId" type="Id" default="" />
    <aura:attribute name="AccountIdByPass" type="Id" default="" />
    <aura:attribute name="defaultValue" type="Map" default="{Address_Line_2__c:'hell'}" />
    <aura:attribute name="isDupByAcc" type="boolean" default="false" />
    <aura:attribute name="isEdit" type="boolean" default="false" />
    <aura:attribute name="isDuplicated" type="boolean" default="false" />
    <aura:attribute name="isCommercialAccount" type="Boolean" default="FALSE" />
    <aura:attribute name="isAccount" type="Boolean" default="FALSE" />
    <aura:attribute name="isAccountedit" type="Boolean" default="FALSE" />
    <aura:attribute name="url" type="String" default="" />
    <aura:attribute name="editFormTextTitle" type="String" default="" />
    <aura:attribute name="editFormTextSubtitle" type="String" default="" />
    <aura:attribute name="duplicatedErrorMessage" type="String" default="" />
    <aura:attribute name="referralErrorMessage" type="String" default="" />
    <aura:attribute name="salutationValue" type="String" default="" />
    <aura:attribute name="AccessPermission" type="Boolean" default="true" />
    <aura:attribute name="errorCheck" type="String" default="true" />
    <aura:attribute name="errorText" type="String" default="" />
    <aura:attribute name="CompleteBypass" type="boolean" default="false" />
    <aura:attribute name="deviceSize" type="Map" default="{
        smallDeviceSize: '12',
        mediumDeviceSize: '6',
        largeDeviceSize: '6',
        size: '12',
    }" />
    <aura:attribute name="activeSections" type="List" default="['A','B','C','D']" />
    <aura:attribute name="spinner" type="boolean" default="FALSE" />
    <aura:attribute name="theme" type="String" default="{!$Browser.formFactor == 'PHONE' ? 'Theme4t' : 'Theme4u'}" />
    <aura:attribute name="density" type="String" default="{!v.theme == 'Theme4t' ? 'comfy' : 'compact'}" />
    <aura:attribute name="error" type="boolean" default="TRUE" />
    <aura:attribute name="fields" type="List" default="['firstName', 'lastName','salutation']" />
    <aura:attribute name="fieldsSalutation" type="List" default="['salutation']" />
    <aura:attribute name="fieldsFirstName" type="List" default="['firstname']" />
    <aura:attribute name="fieldsLastName" type="List" default="['lastname']" />
    <aura:attribute name="salutationOptions" type="List" default="[
        {'label': 'Mr.', 'value': 'Mr.'},
        {'label': 'Ms.', 'value': 'Ms.'},
        {'label': 'Mrs.', 'value': 'Mrs.'},
        {'label': 'Dr.', 'value': 'Dr.'},
        {'label': 'Prof.', 'value': 'Prof.'},
    ]" />
    <aura:handler name="init" value="{!this}" action="{!c.doInit}" />
    <lightning:workspaceAPI aura:id="workspace" />
    <lightning:navigation aura:id="navService" />

