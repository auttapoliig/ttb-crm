<aura:component implements="lightning:isUrlAddressable" access="global"  controller="RemoteVFClass">
    <lightning:navigation aura:id="navService" />
    <lightning:workspaceAPI aura:id="workspace" />
    <lightning:navigationItemAPI aura:id="navigationItemAPI"/>

    <aura:attribute name="recordId" type="Id" />
    <aura:attribute name="vercode" type="Id" />
    <aura:attribute name="objectName" type="String" />

    <aura:attribute name="encTMBID" type="String"/>
    <aura:attribute name="encPhoneNo" type="String"/>
    <aura:attribute name="queueNo" type="String"/>
    <aura:attribute name="branchCode" type="String"/>

    <aura:handler name="init" value="{!this}" action="{!c.onInit}" />
</aura:component>